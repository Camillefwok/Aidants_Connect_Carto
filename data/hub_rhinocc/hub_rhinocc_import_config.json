{
    "data_source_name": "Hub RhinOcc",
    "data_source_type": "hub",
    "name": "Lieux d’inclusion numérique en Occitanie",
    "url": "https://rhinocc-carto-preprod.netlify.app/",
    "file_path": "data/hub_rhinocc/RHINOCC-carto-BDD_export.csv",
    "last_updated": "2020-01-30",
    "import_comment": "5 services différents. Avec pour chacun : type, conditions_acces_si_diff, presta, bloc",
    "import_config": {
        "dataset_file_delimiter": ",",
        "place_fields_mapping_auto": [
            {
                "place_field": "name",
                "file_field": "nom"
            },
            {
                "place_field": "supporting_structure_name",
                "file_field": "structure_porteuse"
            },
            {
                "place_field": "contact_email",
                "file_field": "email_structure"
            },
            {
                "place_field": "contact_website_url",
                "file_field": "site_internet"
            }
        ],
        "place_fields_mapping_process": [
            {
                "place_field": "type",
                "file_field": "type_lieu",
                "comment": "beaucoup de valeurs de l'ordre du nom du lieu"
            },
            {
                "place_field": "legal_entity_type",
                "file_field": "nature_juridique",
                "comment": "parfois préfixé de PUB ou PRV ce qui empêche une bon mapping"
            },
            {
                "place_field": "target_audience_raw",
                "file_field": "public_toutes_actions"
            },
            {
                "place_field": "support_access_raw",
                "file_field": "conditions_acces_toutes_actions"
            },
            {
                "place_field": "address_raw",
                "file_field": "adresse_full"
            },
            {
                "place_field": "contact_phone_raw",
                "file_field": "tel_structure"
            },
            {
                "place_field": "opening_hours_raw",
                "file_field": ["lundi", "mardi", "mercredi", "jeudi", "vendredi", "samedi", "dimanche"]
            }
        ],
        "place_fields_mapping_additional_information": [
            "rh_id",
            "n_ref",
            "statut_infos",
            "code_commune",
            "geo_lat",
            "geo_lon",
            "type_lieu",
            "rattachement",
            "horaires_semaine",
            "date_maj",
            "code_label",
            "label_full",
            "wifi",
            "type_mediation_1",
            "conditions_acces_si_diff_1"
        ]
    }
}
